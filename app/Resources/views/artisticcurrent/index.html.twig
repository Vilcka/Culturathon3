{% extends 'base.html.twig' %}

{% block body %}
    <h1>Artisticcurrents list</h1>

    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for artisticCurrent in artisticCurrents %}
            <tr>
                <td><a href="{{ path('artisticcurrent_show', { 'id': artisticCurrent.id }) }}">{{ artisticCurrent.id }}</a></td>
                <td>{{ artisticCurrent.name }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('artisticcurrent_show', { 'id': artisticCurrent.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('artisticcurrent_edit', { 'id': artisticCurrent.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('artisticcurrent_new') }}">Create a new artisticCurrent</a>
        </li>
    </ul>
{% endblock %}
