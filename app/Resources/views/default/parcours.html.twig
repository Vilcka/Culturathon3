{% extends 'base.html.twig' %}

{% block body %}


    <div class="row mb-5 pb-5 mt-5">

        <div class="col-2 text-center"><img src="img/medal.png"  width="150px" height="150px"></div>
        <div class="col-2 text-center"><img src="img/medal.png" class="medal" width="150px" height="150px"></div>
        <div class="col-2 text-center"><img src="img/medal.png"  class="medal" width="150px" height="150px"></div>
        <div class="col-2 text-center"><img src="img/medal.png"  width="150px" height="150px"></div>
        <div class="col-2 text-center"><img src="img/medal.png" class="medal" width="150px" height="150px"></div>
        <div class="col-2 text-center"><img src="img/medal.png"  class="medal" width="150px" height="150px"></div>






    </div>


<div class="">
    <div class="row">
    {% for current, artworks in artworksByCurrent %}

        <div class="col-4">

    <table class="table">
        <thead class="thead-dark">
        <tr>
            <th scope="col">{{ current }}</th>

        </tr>
        </thead>
        <tbody>
        {% for artwork in artworks %}
        <tr>
            <th scope="row">{{ artwork.title }} {{ artwork.artist.lastName }} {{ artwork.museum.name }} <img src="img/checked.png" width="20px" height="20px"></th>

        </tr>
        {% endfor %}
        </tbody>
    </table>
        </div>
    {% endfor %}
    </div>
    </div>

</div>












{% endblock %}